      "goToPageBottom": [ "shift+g", "meta+down" ],
      "goToFirstInput": "g i",
      "goBack": "ctrl+o",
      "goForward": "ctrl+i",
      "reload": "r",
      "tabForward": [ "ctrl+l"],
      "tabBack": [ "ctrl+h" ],
