#!/bin/bash
#Generated by JetBrains Toolbox 2.6.3.43718 at 2025-06-16T20:33:07

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/dean/Applications/PhpStorm.app/Contents/MacOS/phpstorm" $wait --args "${intellij_args[@]}"